{
  "name": "svelte-reactive-dnd",
  "version": "0.1.1",
  "description": "A drag and drop library for Svelte",
  "svelte": "lib/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:dist && npm run build:lib",
    "build:dist": "rollup -c",
    "build:lib": "node -r esm ./buildlib.js -- --quiet",
    "check": "npm run check:svelte && npm run check:type && npm run check:lint",
    "check:svelte": "svelte-check --ignore g'dist,lib'",
    "check:type": "tsc --noEmit -p ./tsconfig.json",
    "check:lint": "eslint . && prettier --check ./src/**/*.svelte",
    "fix:lint": "eslint . --fix && prettier --write ./src/**/*.svelte",
    "prepublishOnly": "npm run check && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fix:lint",
      "pre-push": "npm run check"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyythane/svelte-reactive-dnd.git"
  },
  "keywords": [
    "svelte",
    "drag",
    "drop",
    "drag-and-drop",
    "dnd",
    "sortable",
    "draggable",
    "responsive",
    "resizable"
  ],
  "author": "Lillian Primorose",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/kyythane/svelte-reactive-dnd/issues"
  },
  "homepage": "https://github.com/kyythane/svelte-reactive-dnd#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@tsconfig/svelte": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.26.10",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.24.1",
    "svelte-check": "^1.0.36",
    "svelte-preprocess": "^4.2.1",
    "svelte2tsx": "^0.1.104",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}